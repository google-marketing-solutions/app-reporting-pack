gaarf:
  output: bq
  api_version: '16'
  account:
  - '{{ account }}'
  params:
    macro:
      start_date: '2024-05-17'
      end_date: '2024-05-19'
      date_iso: '20240520'
      yesterday_iso: '20240519'
      initial_load_date: '2024-05-10'
    template:
      skan4: 'true'
  customer_ids_query: SELECT customer.id FROM campaign WHERE campaign.advertising_channel_type
    = "MULTI_CHANNEL"
  bq:
    project: '{{ project }}'
    dataset: '{{ dataset }}'
gaarf-bq:
  project: '{{ project }}'
  params:
    macro:
      bq_dataset: '{{ dataset }}'
      target_dataset: '{{ dataset }}_output'
      legacy_dataset: '{{ dataset }}_legacy'
      initial_date: '20240510'
      date_iso: '20240520'
      yesterday_iso: '20240519'
      start_date: '2024-05-17'
      current_date: '2024-05-20'
      current_datetime: '2024-05-20 00:00:00'
    template:
      cohort_days: 1,3,5,7,14,30
      has_skan: 'true'
      incremental: 'true'
      skan4: 'true'
scripts:
  video_orientation:
    mode: placeholders
  skan_mode:
    mode: table
backfill: false
incremental: true
legacy: false
